{
  "run_list": [
    "role[packer]",
    "recipe[php]",
    "role[web-nginx]",
    "role[db]"
  ],
  "nginx": {
    "sites": {
      "{{hostname}}": {
        "docroot": "/vagrant/public"
      }
    }
  },
  "mysql": {
    "users": {
      "{{name}}": {
        "password": "{{name}}",
        "database_name": "{{name}}"
      }
    }
  }
}
