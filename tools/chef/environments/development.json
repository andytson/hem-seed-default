{
  "name": "development",
  "description": "",
  "json_class": "Chef::Environment",
  "chef_type": "environment",

  "default_attributes": {
    "nginx": {
      "sites": {
        "{{name}}": {
          "server_name": "{{hostname}}",
          "docroot": "/vagrant/public"
        }
      }
    }
  },

  "override_attributes": {
    "data-bag-merge": {
      "environments": {
        "encrypted": false
      }
    }
  }
}
