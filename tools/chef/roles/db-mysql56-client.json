{
  "name": "db-mysql56-client",
  "chef_type": "role",
  "json_class": "Chef::Role",
  "description": "MySQL 5.6 database client",
  "default_attributes": {
    "mysql-client": {
      "replace_package_target": "Percona-Server-client-56"
    }
  },
  "override_attributes": {
    "mysql": {
      "client": {
        "packages": [
          "Percona-Server-client-56",
          "Percona-Server-devel-56"
        ]
      }
    }
  },
  "run_list": [
    "recipe[yum-percona]",
    "role[db-mysql-upgrade]",
    "recipe[mysql::client]"
  ]
}
